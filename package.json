{
  "name": "daisyui-demo",
  "version": "1.1.1",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "style": "npm i daisyui@latest && postcss node_modules/tailwindcss/tailwind.css -o assets/css/style.css",
    "generate": "NODE_ENV=production npm run style && nuxt generate",
    "predeploy": "NODE_ENV=production npm run style && ROUTER_BASE='/' nuxt generate",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/svg": "^0.1.12",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^9.8.6",
    "core-js": "^3.6.5",
    "cssnano": "^4.1.11",
    "daisyui": "^0.21.0",
    "nuxt": "^2.15.4",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.2",
    "tailwindcss": "^2.0.1-compat",
    "theme-change": "^1.2.0",
    "vue-highlightjs": "^1.3.3"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  }
}
